              <stringProp name="script">String response = prev.getResponseDataAsString();

if (response.contains(&quot;&lt;td&gt;Id&lt;/td&gt;&quot;) &amp;&amp; response.contains(&quot;&lt;/td&gt;&quot;)) {
    int idStart = response.indexOf(&quot;&lt;td&gt;Id&lt;/td&gt;&quot;);
    int valueStart = response.indexOf(&quot;&lt;td&gt;&quot;, idStart + 10) + 4;
    int valueEnd = response.indexOf(&quot;&lt;/td&gt;&quot;, valueStart);
    String id = response.substring(valueStart, valueEnd).trim();

   if (id.equals(&quot;&quot;)||id.length()&lt;10) {
        Failure = true;
        FailureMessage = &quot;Invalid ID found: &quot; + id;
    } else {
        log.info(&quot;Validated Transaction ID: &quot; + id);
        vars.put(&quot;Payment Id&quot;, id);   
    }
} else {
    Failure = true;
    FailureMessage = &quot;Transaction ID tag not found in response.&quot;;
}
