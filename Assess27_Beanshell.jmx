            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor-Destination city">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">String response = prev.getResponseDataAsString();
int start = response.indexOf(&quot;Arrives: &quot;) + 9;
int end = response.indexOf(&quot;&lt;/th&gt;&quot;, start);

String destination = response.substring(start, end).trim();
vars.put(&quot;Destination city&quot;, destination);

String expected=vars.get(&quot;toCity&quot;);
if (!destination.equals(expected)) {
    Failure = true;
    FailureMessage=&quot;Destination city Expected: &quot; + expected + &quot;,but found: &quot; + destination;
} else {
    log.info(&quot;Destination city validated: &quot; + destination);
}

</stringProp>
            </BeanShellPostProcessor>
