              <stringProp name="filename"></stringProp>
              <stringProp name="script">//import java.util.regex.*;
//String response = prev.getResponseDataAsString();
//Pattern pattern = Pattern.compile(&quot;&lt;input type=\\\&quot;hidden\\\&quot; value=\\\&quot;(.*?)\\\&quot; name=\\\&quot;price\\\&quot;&quot;);
//
//Matcher matcher = pattern.matcher(response);
//
//if (matcher.find()) {
//    String flightId = matcher.group(1);
//    vars.put(&quot;flightID&quot;, flightId);
//    log.info(&quot;Extracted Flight ID = &quot; + flightId);
//} else {
//    log.error(&quot;Flight ID not found in response.&quot;);
//}
import java.util.regex.*;
import java.util.ArrayList;

String response = prev.getResponseDataAsString();
Pattern pattern = Pattern.compile(&quot;&lt;input type=\\\&quot;hidden\\\&quot; value=\\\&quot;(.*?)\\\&quot; name=\\\&quot;price\\\&quot;&quot;);
Matcher matcher = pattern.matcher(response);

// Store all matched prices in a list
ArrayList prices = new ArrayList();

while (matcher.find()) {
    prices.add(matcher.group(1)); // group(1) = price like 472.56
}

if (prices.size() &gt; 0) {
    int randomIndex = (int)(Math.random() * prices.size());
    String selectedPrice = (String) prices.get(randomIndex);

    vars.put(&quot;price&quot;, selectedPrice);
    log.info(&quot;Randomly selected price: &quot; + selectedPrice);
} else {
    Failure = true;
    FailureMessage = &quot;No price values found in response.&quot;;
    log.error(&quot;No prices matched for regex.&quot;);
}
</stringProp>
