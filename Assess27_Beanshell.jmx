            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Random from toports">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">import java.util.regex.*;
import java.util.ArrayList;

String response = prev.getResponseDataAsString();
ArrayList allCities = new ArrayList();
ArrayList fromCities = new ArrayList();
ArrayList toCities = new ArrayList();
Pattern pattern = Pattern.compile(&quot;&lt;option value=\\\&quot;(.*?)\\\&quot;&gt;&quot;);
Matcher matcher = pattern.matcher(response);

while (matcher.find()) {
    allCities.add(matcher.group(1));
}
for (int i = 0; i &lt; allCities.size(); i++) {
    if (i &lt; 7) {
        fromCities.add(allCities.get(i));
    } else if (i &lt; 14) {
        toCities.add(allCities.get(i));
    }
}

if (fromCities.size() &gt; 0 &amp;&amp; toCities.size() &gt; 0) {
    String from = (String) fromCities.get((int)(Math.random() * fromCities.size()));
    String to = (String) toCities.get((int)(Math.random() * toCities.size()));

    vars.put(&quot;fromCity&quot;, from);
    vars.put(&quot;toCity&quot;, to);

    log.info(&quot;Selected FROM: &quot; + from + &quot;, TO: &quot; + to);
} else {
    log.error(&quot;City lists are empty or incomplete.&quot;);
}
</stringProp>
            </BeanShellPostProcessor>
