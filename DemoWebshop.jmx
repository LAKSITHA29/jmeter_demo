              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">// Get the response data from the previous request (search/product page)
String response = prev.getResponseDataAsString();

// Pattern: match itemquantity followed by digits
//String regex = &quot;name=\&quot;itemquantity(\\d+)\&quot;&quot;;
String regex = &quot;name=\\\&quot;itemquantity(\\d+)\\\&quot;&quot;;

// Compile regex
java.util.regex.Pattern pattern = java.util.regex.Pattern.compile(regex);
java.util.regex.Matcher matcher = pattern.matcher(response);

// Extract and store in variable
if (matcher.find()) {
    String productId = matcher.group(1);
    vars.put(&quot;proqauntityId&quot;, proqauntityId);
    log.info(&quot;Extracted productId = &quot; + proqauntityId);
} else {
    log.error(&quot;Product ID not found in response!&quot;);
    vars.put(&quot;proqauntityId&quot;, &quot;NOT_FOUND&quot;);
}
</stringProp>
            </BeanShellPreProcessor>
