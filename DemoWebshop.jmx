            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor">
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">try {
    String response = prev.getResponseDataAsString();
    String token = &quot;&quot;;

    int start = response.indexOf(&quot;__RequestVerificationToken&quot;);
    if (start != -1) {
        int valueStart = response.indexOf(&quot;value=\&quot;&quot;, start) + 7; // Finds the start of token value
        int valueEnd = response.indexOf(&quot;\&quot;&quot;, valueStart);        // Finds the end of token value
        token = response.substring(valueStart, valueEnd);         // Extracts token
        vars.put(&quot;reqVerifToken&quot;, token);                         // Saves to variable
        log.info(&quot;Extracted Token: &quot; + token);
    } else {
        log.warn(&quot;Token not found!&quot;);
    }
} catch (Exception e) {
    log.error(&quot;Error while extracting RequestVerificationToken: &quot; + e.getMessage());
}
</stringProp>
            </BeanShellPostProcessor>
