if (response.contains(&quot;Estimate shipping&quot;)) {
    log.info(&quot;Added in cart&quot;);
    vars.put(&quot;addToCartStatus&quot;, &quot;success&quot;);
} else {
    log.error(&quot;not added in cart&quot;);
    vars.put(&quot;addToCartStatus&quot;, &quot;fail&quot;);
}</stringProp>
